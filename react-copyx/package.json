{
  "name": "react-copyx",
  "version": "1.0.5",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify --treeshake --clean",
    "dev": "tsup src/index.ts --watch --format cjs,esm --dts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest"
  },
  "keywords": [
    "react copy",
    "react copy to clipboard",
    "react copy text",
    "copy hook react",
    "copy utility react",
    "clipboard",
    "react hook",
    "copy text"
  ],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9.36.0",
    "husky": "^8.0.0",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "react": ">=17"
  }
